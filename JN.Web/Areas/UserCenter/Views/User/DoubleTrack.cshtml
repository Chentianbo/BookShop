            <div class="row">
                <div class="col-sm-12">
                    <a href="@Url.Action("TreeView")" class="btn btn-primary">推荐图谱</a>
                    @if (JN.Services.Tool.ConfigHelper.GetConfigString("MemberAtlas") == "double")
            {
                        <a href="@Url.Action("DoubleTrack")" class="btn btn-danger">安置图谱</a>
                    }

