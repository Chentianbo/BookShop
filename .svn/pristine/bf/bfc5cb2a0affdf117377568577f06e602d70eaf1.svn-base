select.m-wrap, input[type="file"].m-wrap {height: 34px !important;line-height: 30px;}
.row-fluid .span5{ width: 44.444%;}
.row-fluid .span5,.row-fluid .span3{ height: 34px; line-height: 34px; border-radius: 3px; padding: 0 2%;}
.form-actions {margin-left:180px;}
.form-actions button,button.btn{background: #2f261d;
    color: #f7f3f3;
    border-radius: 4px;
    border: 1px solid #2d241a;}
.help-hint { padding-left: 180px; color: red; line-height: 24px;}
.searchbar {
    overflow:hidden;
}
@media (min-width: 1200px){
	.rightPanel{
		min-height: 100%;
		/*position: absolute;*/
		left: 260px;
	}
}

@media (max-width: 1199px){
	.leftPanel{
		overflow-y: auto;
		float: none;
		width: 100%;
		min-height: initial;
		max-height: 300px;
		padding-bottom: 15px;
		border-bottom: 1px solid #e8e8e8;
	}
	.rightPanel{
		float: none;
		width: 100%;
		border-left: 0;
		min-height: initial;
	}
	.res-right {float:right !important;}
}

@media (min-width: 992px) {
	.res-announce-right {
		float:right;
	}
}

@media (max-width: 992px){}

@media (min-width: 768px) {	
	.navbar-collapse {
	    float:left!important;
	}
}

@media (max-width: 767px){
	.navbar-toggle .icon-bar{background:#fff;}
	
	nav[role="main-navigation"]{
		float: none;
		margin: 0 !important;
	}
	
.modal-sm{ width:330px !important;}
	
	ul[role="sub-navigation"]{text-align: right; margin: 0;}
	ul[role="sub-navigation"] > li{display: inline-block;}
	ul[role="sub-navigation"] .open .dropdown-menu{
		position: absolute;
		float: left;
		left: auto;
		right: 0;
		background-color: #003D74;
	}
	.topnavs{ top: -84px !important;width: auto !important; background: none !important}
	.topnavs .dropdown-menu li{ width: 100% !important;}
	.topnavs .navbar-nav{ margin:7.5px 5px !important; }
	.topnavs li{ display: inline-block; vertical-align: top;}
		ul[role="sub-navigation"] .open .dropdown-menu li a{
			text-align: left;
			padding: 5px 15px;
		}
	#donationWrap .btn#pdBtn{margin-bottom: 0px;}
	.fileupload .fa-file,.helpbtns em{display: none}

	.page-title .title{float:none;}
	ul.walletNav{
		display: block;
		float: none;
		border: 0px solid #FFFFFF;
		margin-bottom: 0;
	}
	ul.walletNav li{
		width: 46%;
		padding: 52px 10px;
		margin-left: 10px;
		vertical-align: middle;
		height: 190px;
	}

        ul.walletNav li:first-child {
            margin-left: 10px;
        }
	ul.walletNav li .wallet{float: right;}
	
	#lang_nav ul {
	    min-width: 105px;
	}
	
	#lang_nav ul li {
		text-align: left;
	}
	
	.navbar-nav>li>.dropdown-menu {
		background: #F5A800 !important;
	}
	
	.navbar-nav>li>.walletNav{
		background: #003D74;
		border: 1px solid #008fcf !important;
	}
	
	.allbox-shadow {
		-webkit-box-shadow: 0 5px 5px 0 rgba(0,0,0,0.5) !important;
		box-shadow: 1px 1px 2px 0 rgba(0,0,0,0.5) !important;
	}
	
	#head-navbar-collapse{width: 100%;position: absolute;right: 0;z-index: 999;height: auto;background: #009ad6;}
	#head-navbar-collapse .dropdown-menu{width: 100%;overflow: hidden;}
	#head-navbar-collapse .dropdown-menu li{width: 119px;}
	#head-navbar-collapse > ul{text-align: left;}
	#tuiguang-link{padding-top:0px;width: 100%;}
	ul[role="sub-navigation"]{margin-top: 5px;}
	.leftbox,.rightbox{ width:100%; float:none;}
	.form-horizontal .control-label{ display:block;text-align: left;}
	.form-horizontal .controls{ margin-left:158px;}
	.row-fluid .span5{ width: 100%; margin-bottom: 10px;}
	.row-fluid{ margin-top: 15px;}
}

@media (max-width: 480px) {
    .innerContent {
    }

    .widget {
        margin: 0 auto 15px;
        position: relative;
        border-radius: 5px;
    
        border: 0px solid #f6b92e;
    }

        .widget .widget-head {
            background-color: #009AD6;
        }
}

.xinxi a{
    padding-left:10px;
}
.xinxi a:hover
{
    text-decoration:none;
}