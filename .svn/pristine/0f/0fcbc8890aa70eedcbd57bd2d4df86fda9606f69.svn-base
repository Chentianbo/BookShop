/* CSS Document */
body,h1,h2,h3,h4,h5,h6,hr,p,blockquote,dl,dt,dd,ul,ol,li,pre,form,fieldset,legend,button,textarea,th,td,img{border:0;margin:0;padding:0;}
em{font-style:normal;list-style:none;}
html{overflow:scroll;overflow-x:hidden;}
body{background:#d6dedf; font-size:12px;font-family:"ËÎÌå",Arial, Helvetica, sans-serif; color:#333; margin-bottom:10px;}
.clear0{ clear:both; height:0; overflow:hidden;}
.clear5{ clear:both; height:5px; overflow:hidden;}
.clear10{ clear:both; height:10px; overflow:hidden;}
.clear15{ clear:both; height:15px; overflow:hidden;}
.clear20{ clear:both; height:20px; overflow:hidden;}
th,td,input{font-size:12px;}
html,body {overflow-x:hidden;SCROLLBAR-FACE-COLOR: #ececec;SCROLLBAR-HIGHLIGHT-COLOR: #ececec;SCROLLBAR-SHADOW-COLOR: #ececec;SCROLLBAR-3DLIGHT-COLOR: #ececec;SCROLLBAR-ARROW-COLOR: #3570b8;SCROLLBAR-TRACK-COLOR: #fefefe;SCROLLBAR-DARKSHADOW-COLOR:#cfcfcf;
SCROLLBAR-BASE-COLOR: #ececec;}
a{text-decoration:none; color:#333;}
a:hover{text-decoration:none;color:#f00;}
a:hover {star:expression(this.onFocus=this.blur());}
a:active {star:expression(this.onFocus=this.blur());}
:focus {outline:0; }
ul{margin:0;padding:0;}
@font-face {font-family: "aaa"; src: url( ../font/msyh.eot ); /* IE */ src: local("Î¢ÈíÑÅºÚ"), url( ../font/msyh.ttf); /* ·ÇIE */}

