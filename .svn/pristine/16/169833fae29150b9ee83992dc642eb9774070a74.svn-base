@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<!--<![endif]--><!-- BEGIN HEAD -->
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>@T(ViewBag.Title)</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="description">
    <meta content="" name="author">
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link rel="stylesheet" href="/theme/mmm/css/font-awesome.css">
    <link rel="stylesheet" href="/theme/mmm/css/metisMenu.css">
    <link rel="stylesheet" href="/theme/mmm/css/animate.css">
    <link rel="stylesheet" href="/theme/mmm/css/bootstrap.css">

    <!-- App styles -->
    <link rel="stylesheet" href="/theme/mmm/css/pe-icon-7-stroke.css">
    <link rel="stylesheet" href="/theme/mmm/css/helper.css">
    <link rel="stylesheet" href="/theme/mmm/css/style.css">
    <!-- END PAGE LEVEL STYLES -->
    <link rel="shortcut icon" href="/theme/mmm/css/favicon.ico">
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="login">
    <!-- BEGIN LOGO -->
    <div class="logo" style="width: 100%;">
        <!--<img src="/assets/img/logo-big.png" alt="" />-->

    </div>
    <!-- END LOGO -->
    <!-- BEGIN LOGIN -->
    <div class="content">
        <!-- BEGIN LOGIN FORM -->
        <form class="form-vertical login-form" id="Success">
            <h3 class="erro" font="">退出成功</h3>
            <div class="loading">
                <div class="splash">
                    <div class="color-line"></div><div class="splash-title">
                        <h1>@T(MvcCore.Unity.Get<JN.Data.Service.ISysSettingService>().Single(1).SysName)  </h1>@*<p>@T("世界上最便捷的财富管理平台") </p>*@
                        <h3>@T(ViewBag.msg)</h3>
                    @*<img src="/theme/mmm/images/loading-bars.svg" width="64" height="64"> <br>*@
                        The system will automatically return in <span id="ShowDiv">0</span>
                        seconds,or click <a href="/usercenter/home">Here</a> to return
                    <input  type="hidden" id="url" value="@ViewBag.url"/>
                    </div>
                </div>


                <!-- END LOGIN FORM -->

            </div>
        </form>
        <!-- END LOGIN -->
        <!-- BEGIN COPYRIGHT -->
        <div class="copyright">
            2015@copyrights. @T(MvcCore.Unity.Get<JN.Data.Service.ISysSettingService>().Single(1).SysName) Alright Reserved.
        </div>
        <!-- END COPYRIGHT -->
        <!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
        <!-- BEGIN CORE PLUGINS -->
        <script type="text/javascript" async="" src="/theme/mmm/js/_Incapsula_Resource"></script>
        <script src="/theme/mmm/js/jquery-1.10.1.min.js" type="text/javascript"></script>
        <script src="/theme/mmm/js/jquery-migrate-1.2.1.min.js" type="text/javascript"></script>
        <!-- IMPORTANT! Load jquery-ui-1.10.1.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
        <script src="/theme/mmm/js/jquery-ui-1.10.1.custom.min.js" type="text/javascript"></script>
        <script src="/theme/mmm/js/bootstrap.min.js" type="text/javascript"></script>
        <!--[if lt IE 9]>
        <script src="/assets/plugins/excanvas.min.js"></script>
        <script src="/assets/plugins/respond.min.js"></script>
        <![endif]-->
        <script src="/theme/mmm/js/jquery.slimscroll.min.js" type="text/javascript"></script>
        <script src="/theme/mmm/js/jquery.blockui.min.js" type="text/javascript"></script>
        <script src="/theme/mmm/js/jquery.cookie.min.js" type="text/javascript"></script>
        <script src="/theme/mmm/js/jquery.uniform.min.js" type="text/javascript"></script>
        <!-- END CORE PLUGINS -->
        <!-- BEGIN PAGE LEVEL PLUGINS -->
        <script src="/theme/mmm/js/jquery.validate.min.js" type="text/javascript"></script>
        <!-- END PAGE LEVEL PLUGINS -->
        <!-- BEGIN PAGE LEVEL SCRIPTS -->
        <script src="/theme/mmm/js/app.js" type="text/javascript"></script>
        <script src="/theme/mmm/js/login.js" type="text/javascript"></script>
        <!-- END PAGE LEVEL SCRIPTS -->
        <script>
            jQuery(document).ready(function () {
                App.init();
                //Login.init();
            });
        </script>
        <!-- END JAVASCRIPTS -->
        <!-- END BODY -->
        <script language="javascript" type="text/javascript">

            var secs = 1;
            var jumpUrl = $("#url").val();
            function doUpdate(num) {
                document.getElementById('ShowDiv').innerHTML = num;
                if (num == 0) window.location = jumpUrl;
            }
            $(window).load(function () {

                for (var i = secs; i >= 0; i--) {
                    window.setTimeout('doUpdate(' + i + ')', (secs - i) * 1000);
                }

            });
            /*(function(){
                var secs =5;
                var jumpUrl="/";
                function loads(url){
                    for(var i=secs;i>=0;i--){
                        window.setTimeout('doUpdate(' + i + ')', (secs-i) * 1000);
                    }
                }

                function doUpdate(num){
                    document.getElementById('ShowDiv').innerHTML = num;
                    if(num == 0)  window.location=jumpUrl;
                }

            })();*/

        </script>


    </div>
</body>
</html>