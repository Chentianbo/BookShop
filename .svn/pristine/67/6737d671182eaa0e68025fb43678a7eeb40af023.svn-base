/*new update here*/
.navbar-center {
	position:relative; 
	text-align:center; 
	width:100%; 
	height:100%; 
	margin-top:1%;
}

.navbar-center img{
    position: absolute;
    margin: auto;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}
.navbar-collapse {box-shadow: inset 0 0px 0 rgba(255,255,255,.1);}

.collapsing{
	position:relative;
	height:0;
	overflow:hidden;
	-webkit-transition:height .35s ease;
	transition:height .35s ease
}
/*end update*/

