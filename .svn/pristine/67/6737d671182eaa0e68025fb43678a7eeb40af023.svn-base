/* Alert ============== */
.alert{font-size: 12px;}
	.alert.alert-danger {
		background: #bd362f;
		color: #fff;
		border-color: #bd362f;
	}
		.alert.alert-danger a{color: #FFF; text-decoration: underline}
	.alert.alert-success {
		border-color: #609450;
		background: #609450;
		color: #fff;
	}
	.alert.alert-warning {
