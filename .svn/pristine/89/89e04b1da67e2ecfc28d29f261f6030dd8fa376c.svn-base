	background-size:100% 100%;
	}

.big_window .dd2 a:hover{background:url(../images/close2.png) no-repeat;background-size:100% 100%;}
	
	}


.member_message {
    margin:20px auto;
    }


.member_message .logo a{
    display:block;
    border:none;    
    }

.member_message .logo a img{
    display:block;
    border:none;    
    }

.message h1{
    width: 100%;
    padding: 0;
    margin: 0;
    display: inline-block;
    font-weight: 400;
    font-size: 24px;
    border-bottom: 1px solid rgba(255, 255, 255, 0.3);
    height: 40px;
    box-shadow: 0 3px 0 rgba(0, 0, 0, 0.05);
    margin-top: 30px;
    }

.member_message .message ul {
       overflow:hidden;
       margin:10px auto;
    }


.member_message .message ul li{
       float:left;
       margin-right:40px;
    }

.member_message .message ul li a{
       display:inline-block;
       font-size: 14px;
        font-weight: 600;
    }

.member_message .message ul li .a2{
       color:rgb(255, 245, 110);
       font-weight:normal;
    }

.list_box {
    overflow:hidden;
    }

.list_box .offer_list_box{
    float:left;
    width:49%;
    background: rgba(0, 0, 0, 0.3);
    border-radius: 4px;
    margin: 20px 0px 0px 0px;
    padding: 10px
    }

.list_box .get_list_box{
    float:right;
    width:49%;
    background: rgba(0, 0, 0, 0.3);
    border-radius: 4px;
    margin: 20px 0px 0px 0px;
    padding: 10px
    }

.list_box div h2 {
    font-weight: 400;
    font-size: 24px;
    margin-bottom: 10px;
    }

.list_box div table {
    width:100%;
    border:1px solid #c0c1c4;
    }

.list_box div table tr{  
    border-bottom:1px solid #c0c1c4;
    }

.list_box div table th,.list_box div table td{  
    border-right:1px solid #c0c1c4;
    height: 34px;
    text-align: center;
    vertical-align: middle;
    white-space: nowrap;
    }

.list_box div table th:last-child,.list_box div table td:last-child{  
    border-right:none;
    }


.message .wallet_box{
       overflow:hidden;
       margin:10px auto;
    }

.message .wallet_box dl{
       float:left;
       width:22%;
       margin-left:2.5%;
       background:url(/Theme/mmm/images/cart.png) no-repeat 10px center #418bca;
       padding: 10px 10px 10px 80px;
       height: 115px;
       border-radius: 8px;
    }

.message .wallet_box dl dt{
    margin-top: 10px;
    font-size: 14px;
    color: rgba(255, 255, 255, 0.5);
    }

.message .wallet_box dl dd{
    font-size: 30px;
    }

.message .wallet_box dl:first-child{      
       background:url(/Theme/mmm/images/cart.png) no-repeat 10px center #d9544f;
    }

.message .wallet_box dl:last-child{      
       background:url(/Theme/mmm/images/cart.png) no-repeat 10px center #7DBD88;
    }


.text_scroll {
    width:60%;
    float:left;
    }

.text_scroll a {
    display:block;
    float:left;
    line-height:46px;
    height:46px;
    margin-right:100px;
    }


.text_scroll a:hover{
    text-decoration:underline;
    }


@media(max-width:1100px) {
.member_message .message ul li{
       float:none;
       margin-right:0px;
    }


.list_box .offer_list_box{
    float:none;
    width:100%;
    overflow:auto;
    background: rgba(0, 0, 0, 0.3);
    border-radius: 4px;
    margin: 20px 0px 0px 0px;
    padding: 10px
    }

.list_box .get_list_box{
    float:none;
    width:100%;
    overflow:auto;
    background: rgba(0, 0, 0, 0.3);
    border-radius: 4px;
    margin: 20px 0px 0px 0px;
    padding: 10px
    }

.message .wallet_box dl{
    float: none;
    width: 100%;
    margin-left: 0;
   
}

.text_scroll {
    width: 80%;
}

.text_scroll a {
    display: block;
    float: left;
    line-height: 46px;
    height: 46px;
     margin-right: 10px; 
}

}